---
const {title, font} = Astro.props;
    
import stuha from '@assetsLocal/stuha.png';
---

<div class="flex flex-col space-y-2">
    <h6>{title}</h6>
    <div class=`w-60 h-10 bg-contain bg-repeat bg-top flex ribbon-effect ${font}` style={{backgroundImage: `url('${stuha.src}')`,}}>
        <span class="text-white self-center mx-4 text-2xl name">Meno</span>
    </div>
</div>